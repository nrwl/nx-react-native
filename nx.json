{
  "npmScope": "nrwl",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "detox": {
      "tags": []
    },
    "detox-e2e": {
      "tags": [],
      "implicitDependencies": ["detox"]
    },
    "react-native": {
      "tags": []
    },
    "react-native-e2e": {
      "tags": [],
      "implicitDependencies": ["react-native"]
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  }
}
